# Environment Variables for CMU Playground
# Copy this file to .env and fill in your actual values
# IMPORTANT: Never commit the actual .env file to version control

# LLM API Keys for Real-Time Agent Interactions
VITE_OPENAI_API_KEY=your-openai-api-key-here
VITE_ANTHROPIC_API_KEY=your-anthropic-api-key-here
VITE_COHERE_API_KEY=your-cohere-api-key-here
VITE_HUGGINGFACE_API_KEY=your-huggingface-api-key-here

# Firebase Configuration (if needed)
VITE_FIREBASE_API_KEY=your-api-key-here
VITE_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
VITE_FIREBASE_APP_ID=your-app-id
VITE_FIREBASE_MEASUREMENT_ID=your-measurement-id

