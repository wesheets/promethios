{
  "rewrites": [
    { "source": "/content/(.*)", "destination": "/content/$1" },
    { "source": "/(.*\\.js)", "destination": "/$1", "headers": { "Content-Type": "application/javascript" } },
    { "source": "/(.*\\.css)", "destination": "/$1", "headers": { "Content-Type": "text/css" } },
    { "source": "/(.*\\.png)", "destination": "/$1", "headers": { "Content-Type": "image/png" } },
    { "source": "/(.*\\.jpg)", "destination": "/$1", "headers": { "Content-Type": "image/jpeg" } },
    { "source": "/(.*\\.svg)", "destination": "/$1", "headers": { "Content-Type": "image/svg+xml" } },
    { "source": "/(.*\\.ico)", "destination": "/$1", "headers": { "Content-Type": "image/x-icon" } },
    { "source": "/(.*\\.woff)", "destination": "/$1", "headers": { "Content-Type": "font/woff" } },
    { "source": "/(.*\\.woff2)", "destination": "/$1", "headers": { "Content-Type": "font/woff2" } },
    { "source": "/(.*\\.ttf)", "destination": "/$1", "headers": { "Content-Type": "font/ttf" } },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
